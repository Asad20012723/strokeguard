# Stroke Monitoring API Environment Variables

# API Configuration
API_V1_PREFIX=/api/v1
PROJECT_NAME="Stroke Monitoring API"
DEBUG=false

# CORS - JSON array of allowed origins
# For production, replace with your Vercel domain
CORS_ORIGINS=["http://localhost:3000","https://your-app.vercel.app"]

# Model Configuration
MODEL_PATH=./models/multimodal_model.pth
MODEL_DEVICE=cpu

# Rate Limiting
RATE_LIMIT_REQUESTS=10
RATE_LIMIT_WINDOW=60

# Optional: API Key for additional security
# API_KEY=your-secret-api-key

# Logging
LOG_LEVEL=INFO
